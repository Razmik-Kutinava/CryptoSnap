{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/bot.mjs": {
      "runtime": "nodejs18.x"
    },
    "api/setup-webhook.mjs": {
      "runtime": "nodejs18.x"
    }
  }
}